<div class="page-header">
  <%= link_to guests_path, class: 'btn btn-default' do %>
    All Guests
  <% end %>
  <%= link_to edit_guest_path(@guest), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show guest</h1>
</div>

<dl class="dl-horizontal">
  <dt>Display Name:</dt>
  <dd><%= @guest.title %> <%= @guest.name %></dd>
  <dt>Table:</dt>
  <dd><%= link_to(@guest.table.name, table_path(@guest.table)) %></dd>
  <br>
  <dt>Title:</dt>
  <dd><%= @guest.title %></dd>
  <dt>Name:</dt>
  <dd><%= @guest.name %></dd>
</dl>
